{
  "name": "third-lab",
  "version": "0.6.0",
  "description": "A notebook",
  "author": "Jack Zhu",
  "homepage": "https://github.com/weiweiwitch/third-lab",
  "main": "./static/dist/main.js",
  "scripts": {
    "build-main-prod": "webpack --config webpack/webpack.main.prod.config.js",
    "build-renderer-prod": "webpack --config webpack/webpack.renderer.prod.config.js",
    "build-prod": "npm run build-renderer-prod && npm run build-main-prod",
    "start-main-dev": "webpack --config webpack/webpack.main.dev.config.js && electron --inspect ./static/dev/main.js",
    "start-renderer-dev": "node webpack/webpack-devserver.js",
    "run-prod": "electron ./static/dist/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -w --x64"
  },
  "build": {
    "productName": "thirdlab",
    "appId": "xyz.ariane.thirdlab",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "dist"
    },
    "files": [
      "static/dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": false
    }
  },
  "license": "MIT",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.17",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.1.0",
    "markdown-it-anchor": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6"
  },
  "devDependencies": {
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "@types/superagent": "^3.5.8",
    "antd": "3.5.4",
    "awesome-typescript-loader": "^5.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression": "^1.6.0",
    "css-loader": "^0.23.1",
    "electron": "2.0.10",
    "electron-builder": "^20.28.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^0.11.2",
    "highlight.js": "^9.1.0",
    "history": "^4.7.2",
    "http-proxy": "^1.12.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.3.1",
    "marked": "^0.3.6",
    "multireducer": "^2.0.0",
    "node-sass": "4.9.0",
    "piping": "^0.3.0",
    "postcss": "^5.1.1",
    "pretty-error": "^1.2.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.10",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-form": "5.2.5",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^6.0.6",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "superagent": "^3.8.2",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.8.4",
    "url-loader": "^0.5.7",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.4"
  }
}
